#include <stdio.h>

int main() {
    int amount, notes;
    int denominations[] = {500, 200, 100, 50, 20, 10, 5, 2, 1};
    int i;

    printf("Enter amount: ");
    scanf("%d", &amount);

    printf("\nNotes required:\n");
    for(i = 0; i < 9; i++) {
        notes = amount / denominations[i];
        if(notes > 0) {
            printf("%d Ã— %d\n", notes, denominations[i]);
            amount %= denominations[i];
        }
    }

    return 0;
}
