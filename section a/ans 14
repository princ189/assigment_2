#include <stdio.h>

int main() {
    int arr[10], n, newnum, i;

    printf("Enter number of elements (max 9): ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter new number to insert at last: ");
    scanf("%d", &newnum);

    arr[n] = newnum;
    n++;

    printf("\nArray after insertion:\n");
    for(i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
