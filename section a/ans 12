#include <stdio.h>

int main() {
    int arr[10], n, i, newnum;

    printf("Enter number of elements (max 9): ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter new number to insert at beginning: ");
    scanf("%d", &newnum);

    for(i = n; i > 0; i--)
        arr[i] = arr[i - 1];
    arr[0] = newnum;
    n++;

    printf("\nArray after insertion:\n");
    for(i = 0; i < n; i++)
        printf("%d ", arr[i]);

    return 0;
}
