#include <stdio.h>
#include <string.h>

int main() {
    char str1[100], str2[100];
    int cmp;

    printf("Enter first string: ");
    gets(str1);
    printf("Enter second string: ");
    gets(str2);

    cmp = strcmp(str1, str2);

    if(cmp == 0)
        printf("Strings are same.");
    else if(cmp > 0)
        printf("First string is greater.");
    else
        printf("Second string is greater.");

    return 0;
}
